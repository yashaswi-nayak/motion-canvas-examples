{
  "name": "examples",
  "private": true,
  "description": "A set of examples for Motion Canvas",
  "license": "MIT",
  "scripts": {
    "smooth-parallax": "npm run serve -w examples/smooth-parallax",
    "deferred-lighting": "npm run serve -w examples/deferred-lighting",
    "motion-canvas": "npm run serve -w examples/motion-canvas",
    "basic-shapes": "npm run serve -w examples/basic-shapes",
    "basic-animation": "npm run serve -w examples/basic-animation",
    "logo": "npm run serve -w examples/logo",
    "lint": "eslint \"examples/**/src/**/*.ts?(x)\"",
    "lint:fix": "eslint --fix \"examples/**/src/**/*.ts?(x)\"",
    "prettier": "prettier --check \"examples/**/src/**/*.ts?(x)\"",
    "prettier:fix": "prettier --write \"examples/**/src/**/*.ts?(x)\""
  },
  "workspaces": [
    "examples/*"
  ],
  "dependencies": {
    "@motion-canvas/2d": "^3.6.2",
    "@motion-canvas/core": "^3.6.1"
  },
  "devDependencies": {
    "@motion-canvas/ui": "^3.6.1",
    "@motion-canvas/vite-plugin": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "vite": "^4.3.5"
  }
}